package day02_SWEA;

import java.util.Scanner;

public class SWEA_sum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[][] square = new int[100][100];
		int sum = 0;
		for(int i = 0; i < 100; i++) {
			for(int j = 0; j < 100; j++) {
				square[i][j] = sc.nextInt();
				sum = sum + square[i][j];
				System.out.println(sum + " ");
			}
		}
	}

}
